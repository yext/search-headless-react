{
  "compilerOptions": {
    "baseUrl": ".",
    "paths": {
      "@yext/answers-headless-react": ["src"]
    },
    "esModuleInterop": true,
    "strict": true,
    "skipLibCheck": true,
    "noImplicitAny": false,
    "moduleResolution": "node",
    "forceConsistentCasingInFileNames": true,
    "declaration": true,
    "declarationMap": true,
    "sourceMap": true,
    "jsx": "react-jsx",
    "resolveJsonModule": true
  },
  "include": [
    "src"
  ]
}